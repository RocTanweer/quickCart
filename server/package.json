{
  "name": "quick_cart_backend",
  "version": "1.0.0",
  "description": "This is backend of quickcart e commerce app",
  "main": "index.server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon index.server.js",
    "start": "node index.server.js",
    "knex:init": "npx knex init",
    "migrate:make": "npx knex --knexfile ./config/knexfile.js migrate:make",
    "migrate:latest": "npx knex --knexfile ./config/knexfile.js migrate:latest",
    "migrate:rollback": "npx knex --knexfile ./config/knexfile.js migrate:rollback",
    "migrate:up": "npx knex --knexfile ./config/knexfile.js migrate:up",
    "migrate:down": "npx knex --knexfile ./config/knexfile.js migrate:down",
    "seed:make": "npx knex --knexfile ./config/knexfile.js seed:make",
    "seed:run": "npx knex --knexfile ./config/knexfile.js seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RocTanweer/quickCart.git"
  },
  "keywords": [
    "e commerce",
    "mysql",
    "expressjs",
    "nodejs",
    "jwt",
    "restapi"
  ],
  "author": "Roc Tanweer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RocTanweer/quickCart/issues"
  },
  "homepage": "https://github.com/RocTanweer/quickCart#readme",
  "devDependencies": {
    "nodemon": "^3.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.9.1"
  }
}
